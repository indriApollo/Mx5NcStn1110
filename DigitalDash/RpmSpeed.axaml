<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DigitalDash.RpmSpeed">
    <Grid ColumnDefinitions="Auto,Auto,Auto" RowDefinitions="Auto,Auto">
        <TextBlock Grid.Row="0" Grid.Column="0">RPM</TextBlock>
        <TextBlock Name="Rpm" Grid.Row="0" Grid.Column="1">000</TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="0">Speed</TextBlock>
        <TextBlock Name="Speed" Grid.Row="1" Grid.Column="1">000</TextBlock>
        <TextBlock Grid.Row="1" Grid.Column="2">Km/h</TextBlock>
    </Grid>
</UserControl>
